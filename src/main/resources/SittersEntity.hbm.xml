<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.grandcircus.spring.models.SittersEntity" table="SITTERS" schema="PETSITTER">

        <id name="sitID" column="sitID">
            <!-- <generator class="assigned"></generator> -->
            <generator class="native" />
        </id>


        <property name="smDog">
            <column name="SmDog" sql-type="tinyint(1)"/>
        </property>
        <property name="medDog">
            <column name="MedDog" sql-type="tinyint(1)"/>
        </property>
        <property name="bigDog">
            <column name="BigDog" sql-type="tinyint(1)"/>
        </property>
        <property name="cat">
            <column name="Cat" sql-type="tinyint(1)"/>
        </property>
        <property name="inHome">
            <column name="InHome" sql-type="tinyint(1)"/>
        </property>
        <property name="temper">
            <column name="Temper" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="feed">
            <column name="Feed" sql-type="tinyint(1)"/>
        </property>
        <property name="walk">
            <column name="Walk" sql-type="tinyint(1)"/>
        </property>
        <property name="play">
            <column name="Play" sql-type="tinyint(1)"/>
        </property>
        <property name="visit">
            <column name="Visit" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>