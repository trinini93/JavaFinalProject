<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.grandcircus.spring.models.ParentPetFormEntity" table="ParentPetForm" schema="PETSITTER">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="UserID" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="petName">
            <column name="petName" sql-type="varchar(140)" length="140" not-null="true"/>
        </property>
        <property name="breeds">
            <column name="breeds" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="medType">
            <column name="medType" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="petWatch">
            <column name="petWatch" sql-type="varchar(140)" length="140" not-null="true"/>
        </property>
        <property name="activity">
            <column name="activity" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>